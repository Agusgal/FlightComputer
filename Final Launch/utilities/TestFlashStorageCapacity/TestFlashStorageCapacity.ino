#include "Logger.h"
#include <nvs_flash.h>
#include <Preferences.h>

Preferences preferences;

void setup() {
    Serial.begin(9600);

    Logger::log("Erasing all flash data");

    nvs_flash_erase();      // erase the NVS partition and...
    nvs_flash_init();       // initialize the NVS partition.

    
    preferences.begin("lanzamiento00", false);

    Logger::log("Storign sample data...");
    // 3000 bytes
    preferences.putString("test1", "");
    // 3000 bytes
    preferences.putString("test2", "");
    // 3000 bytes
    preferences.putString("test3", "");

    Logger::log("Retrieving sample data...");

    Logger::log(preferences.getString("test1", "NO-DATA"));
    Logger::log(preferences.getString("test2", "NO-DATA"));
    Logger::log(preferences.getString("test3", "NO-DATA"));
}

